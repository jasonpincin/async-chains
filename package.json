{
    "name": "async-chains",
        "description": "Asynchronous callback chains with support for caolan's async module",
        "version": "0.0.0",
        "repository": {
            "type": "git",
            "url": "https://github.com/jasonpincin/async-chains"
        },
        "homepage": "https://github.com/jasonpincin/async-chains",
        "keywords": [
            "async",
            "chain",
            "flow",
            "control",
            "callback",
            "emit",
            "EventEmitter"
        ],
        "main": "./index.js",
        "scripts": {
            "test": "jasmine-node test",
            "coverage": "istanbul cover --print summary --dir ./test/.coverage jasmine-node -- test 2> /dev/null || echo 'Istanbul required for test coverage reports: npm install -g istanbul'"
        },
        "engine": {
            "node": ">=0.8.8"
        },
        "author": {
            "name": "Jason Pincin",
            "email": "jason@pincin.com"
        },
        "devDependencies": {
            "jasmine-tapreporter": "~0.2.2",
            "jasmine-node": "~1.10.1",
            "async": "~0.2.9"
        },
        "testling": {
            "files": "test/*_spec.js",
            "browsers": {
                "ie"      : [ 8,9,10 ],
                "firefox" : [ 17 ],
                "chrome"  : [ 20 ],
                "safari"  : [ 6 ],
                "opera"   : [ 12 ]
            }
        },
        "license": "BSD"
}
